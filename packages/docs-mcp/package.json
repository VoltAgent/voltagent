{
  "name": "@voltagent/docs-mcp",
  "version": "0.1.0",
  "description": "VoltAgent docs-mcp - MCP Docs ",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
