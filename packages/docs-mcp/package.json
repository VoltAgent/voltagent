    "test": "vitest --passWithNoTests",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@voltagent/core": "^0.1.31",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "tsup": "^8.5.0",
    "typescript": "^5.0.4",
    "vitest": "^3.2.4"
  }
