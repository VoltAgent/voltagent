  "version": "1.0.0-next.1",
  "dependencies": {
    "@libsql/client": "^0.15.0",
    "@voltagent/internal": "^0.0.9",
    "@voltagent/logger": "^1.0.0-next.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@voltagent/core": "^1.0.0-next.2",
    "ai": "^5.0.12",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  },
  "exports": {
