{
  "name": "@voltagent/rag",
  "version": "0.0.1",
  "description": "Chunking and RAG utilities for VoltAgent",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "dependencies": {
    "js-tiktoken": "^1.0.14",
    "tree-sitter": "^0.25.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --reporter=hanging-process --reporter=default --typecheck",
    "test:coverage": "vitest run --coverage --typecheck"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "devDependencies": {
    "vitest": "^3.2.4"
  }
}
