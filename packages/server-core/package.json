{
  "name": "@voltagent/server-core",
  "description": "Framework-agnostic server core for VoltAgent",
  "version": "1.0.18",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@voltagent/core": "workspace:*",
    "@voltagent/internal": "^0.0.12",
    "ai": "^5.0.76",
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.18.1",
    "zod-from-json-schema": "^0.5.0",
    "zod-from-json-schema-v3": "npm:zod-from-json-schema@^0.0.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ws": "^8.18.1",
    "zod": "^3.25.76"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./edge": {
      "import": {
        "types": "./dist/edge.d.mts",
        "default": "./dist/edge.mjs"
      },
      "require": {
        "types": "./dist/edge.d.ts",
        "default": "./dist/edge.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "peerDependencies": {
    "@voltagent/core": "^1.0.0",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "types": "dist/index.d.ts"
}
