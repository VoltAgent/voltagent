{
  "name": "@voltagent/vercel-ai-exporter",
  "description": "OpenTelemetry exporter for VoltAgent observability with Vercel AI SDK",
  "version": "0.1.6",
  "author": "VoltAgent Team",
  "dependencies": {
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@voltagent/sdk": "^0.1.6"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@voltagent/core": "^0.1.73",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ai",
    "llm",
    "observability",
    "opentelemetry",
    "vercel-ai",
    "voltagent"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "peerDependencies": {
    "@voltagent/core": "^0.1.71",
    "ai": "^3.0.0 || ^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voltagentai/voltagent.git",
    "directory": "packages/vercel-ai-exporter"
  },
  "scripts": {
    "attw": "attw --pack",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check src/",
    "lint:fix": "biome check --apply src/",
    "publint": "publint --strict",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "types": "dist/index.d.ts"
}
